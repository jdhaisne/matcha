//- register.pug
extends layout.pug

block content
	h1= title
	#register-form
		form(action='/register' method='POST')
			span pseudo:
				input(type='text' name='pseudo') 
			span password:
				input(type='password' name='password') 
			span verify password:
				input(type='password' name='password_verif')
			input(type='submit' name='submit')